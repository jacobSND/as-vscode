{
	"compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "outDir": "out",
    "lib": [
      "ES2022",
      "dom"
    ],
    "sourceMap": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", ".vscode-test", "webview-ui"]
}
